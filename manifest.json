{
    "manifest_version": 3,
    "name": "Simple Bookmark Extension",
    "version": "1.0",
    "description": "A simple chrome extension to manage bookmarks.",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    }
}